-- pset(self.lastx,self.lasty,8)
-- pset(self.x,self.y,9)
end

function player:update()
 self:animate()
 self.dx *= f
 self.dy += g
 self.running = false
-- move left
 if btn(‚¨ÖÔ∏è) then
  self.flipx = true
  if self.stand then
   self.running = true
  end
 self.dx -= self.acc
 end
-- move right
 if btn(‚û°Ô∏è) then
  self.flipx = false
  if self.stand then
   self.running = true
  end
  self.dx += self.acc
 end
-- jump
 if btnp(‚ùé) and self.stand then
  sfx(1)
  self.dy = -self.boost
  self.stand = false
  self.running = false
 end
 if self.dx < 0 then
  self.dx = limit(self.dx,self.mdx)
  if collision(self, 'left', 0) or
  self.x<=0 then
   self.dx = 0
  end
 elseif self.dx > 0 then
  self.dx = limit(self.dx,self.mdx)
  if collision(self, 'right', 0) then
--  or self.x>=1008 then
   self.dx = 0
  end
 end
 gravity(self)
 self.x += self.dx
 if (self.x<0) self.x = 0
end

function player:animate()
 if self.running then
  if j%5==0 then
   sfx(2)
   self.sp = self.sp == 10 and 8 or 10
   j=5
  end
 elseif self.jumping then
  self.sp = 8
 elseif self.falling then
  self.sp = 12
 else
  if j%5==0 then
   self.sp += self.sp < self.msp and self.h or self.dsp
   j=5
  end
 end
end

function player:fall()
 cx=(self.x/128)*128
 self.x = self.dx<=0 and cx or cx+112
 self.dy-=self.boost
 self.y=96
 self:die()
end

function player:damage()
 self.dy-=2	
 self.dx+=self.flipx and 2 or -2
 self:die()
end

function player:die()
 sfx(3)
 self.lives-=1
 if self.lives<=0 then
  _init()
  state='gameover'
 end
end

function player:hit(o)
 return self.x + (self.w*8) >= o.x
 and self.y + (self.h*8) >= o.y
 and self.x <= o.x + (o.w*8)
 and self.y <= o.y + (o.h*8)
end
