--game
function game_init()
 cx=0 cy=0 zone=40
 p = player:new(8,cy+96)
 poles={
  pole:new(1000,50,0),
--  pole:new(104,50,0),
 }
 enemies={
  enemy:new(0,96,1,.4),
  enemy:new(1,66,1,.2),
 }
 g=.5 f=.85 i=0
end

function game_draw()
 cls(13)
 map()
 camera(cx,cy)
 for e in all(enemies) do
  e:draw()
 end
 p:draw()
 for pl in all(poles) do
  pl:draw()
 end
 draw_hud()
end

function game_update()
 cam(p)
 if j%15==0 then
  tp=flr(rnd(2))
  posx=flr(rnd(1024/64))*128
	 add(enemies, enemy:new(tp,posx,0,.3))
 end
 for e in all(enemies) do
  e:update()
 end
 for pl in all(poles) do
  pl:update()
  pass(pl)
 end
 p:update()
 if p.y>=cy+128 then
  p:die()
  if p.dx<0 then
	  cx=(p.x/128)*128
	  p.x=cx+112
	  p.y=96
  elseif p.dx>0 then
	  cx=flr(p.x/128)*128
	  p.x=cx+8
	  p.y=96
  end
 end 
 update_hud(p)
end

function draw_hud()
 rectfill(cx,0,cx+127,cy+8,6)
 rect(cx,cy,cx+127,cy+8,5)
 print(lives,cx+2,cy+2,8)
 print(score,cx+115,cy+2,5)
 print('fase: '..flr(cy/128)+1,cx+64,cy+2,5)
end

function update_hud(o)
 lives='' score=''
 if o.score<10 then
  score='00'
 elseif o.score<100 do
  score='0'
 end
 score=score..o.score
 for l=1,o.lives do
   lives=lives..'♥'
 end
end

