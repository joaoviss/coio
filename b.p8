end

function pole:update()
 if j%3==0 then
  self.sp=self.sp==94 and 95 or 94
 end
 gravity(self)
end


function pass(o)
 if p.x>=o.x then
  cx=0
  cy+=(o.lvl+1)*128
  p.x=-16
  p.y=cy+96
 end
end

