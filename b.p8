 print(score,cx+115,cy+2,5)
 print('fase: '..flr(cy/128)+1,cx+64,cy+2,5)
end

function update_hud(o)
 lives='' score=''
 if o.score<10 then
  score='00'
 elseif o.score<100 do
  score='0'
 end
 score=score..o.score
 for l=1,o.lives do
   lives=lives..'♥'
 end
end

