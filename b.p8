--enemy
enemy = {}

function enemy:new(type,x,y,acc)
 local o={
	 t=0,
	 dx=0, dy=0,
	 mdx=2, mdy=3,
	 flipx=false,
	 stand=false,
	}
	o.x=x o.y=y
	o.acc=(rnd()-.5)>0 and acc or -acc
	if type==0 then
	 o.sp=32 o.msp=38
	 o.w=14/8 o.h=2 o.delay=3
	elseif type==1 then
	 o.sp=44 o.msp=47
	 o.w=1 o.h=1 o.delay=5
	end
	o.dsp=o.sp-o.msp
	return setmetatable(o, {__index=self})
end

function enemy:draw()
 spr(self.sp, self.x, self.y, self.w, self.h,self.flipx,false)
end

function enemy:update()
 self:animate()
 self.dx *= f
 if self.stand then
  self.dx+=self.acc
 end
 if self.dx < 0 then
  self.flipx = true
  self.dx = limit(self.dx,self.mdx)
 elseif self.dx > 0 then
  self.flipx = false
  self.dx = limit(self.dx,self.mdx)
 end
 gravity(self)
 self.x += self.dx
 if collision(self,'left',0) or
 collision(self,'right',0) or
 self.x<=1 or self.x>=1116 then
  self.x+=(sgn(self.dx)*-1)
  self.dx=0
  self.acc*=-1
 end
end

function enemy:animate()
 if self.t==0 then
  self.sp+=self.sp<self.msp and self.w or self.dsp
  self.t=self.delay
 else
  self.t-=1
 end
 self.sp=ceil(self.sp)
end

function enemy:hit(o)
 return self.x + (self.w*8) >= o.x
 and self.y + (self.h*8) >= o.y
 and self.x <= o.x + (o.w*8)
 and self.y <= o.y + (o.h*8)
end
