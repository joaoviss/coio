--player
player={
 cnt=0, lives=3, score=0,
 sp=0, msp=6,	
 w=2, h=2, dx=0, dy=0,
 mdx=2, mdy=3,
 acc=3, boost=8, spd=1,
 stand=false, jumping=false, 
 falling=false, running=false,
 flipx=false
}

function player:new(x,y)
 local o={x=x,y=y,dsp=self.sp-self.msp}
 return setmetatable(o,{__index=self})
end

function player:draw()
 spr(self.sp,self.x,self.y,2,2,self.flipx)
end

function player:update()
 self:animate()	
 self.dx *= f
 self.dy += g
 self.running = false
-- move left
 if btn(⬅️) then
  self.flipx = true
  if self.stand then
   self.running = true
  end
 self.dx -= self.acc
 end
-- move right
 if btn(➡️) then
  self.flipx = false
  if self.stand then
   self.running = true
  end
  self.dx += self.acc
 end
-- jump
 if btnp(❎) and self.stand then
  sfx(1)
  self.dy = -self.boost
  self.stand = false
  self.running = false
 end
 if self.dx < 0 then
  self.dx = limit(self.dx,self.mdx)
  if collision(self, 'left', 0) or
  self.x<=0 then
   self.dx = 0
  end
 elseif self.dx > 0 then
  self.dx = limit(self.dx,self.mdx)
  if collision(self, 'right', 0) or
  self.x>=1008 then
   self.dx = 0
  end
 end
 gravity(self)
 self.x += self.dx
 if self.x<0 then
  self.x = 0
 end
end

function player:animate()
 if self.running then
  if self.cnt==0 then
   sfx(2)
   self.sp = self.sp == 10 and 8 or 10
   self.cnt=3
  else
   self.cnt-=1
  end
 elseif self.jumping then
  self.sp = 8
 elseif self.falling then
  self.sp = 12
 else
  if self.cnt==0 then
   self.sp += self.sp < self.msp and self.h or self.dsp
   self.cnt=5
  else
   self.cnt-=1
  end
 end
end

function player:die()
 sfx(3)
 self.lives-=1
 if self.lives<=0 then
  _init()
  state		='gameover'
 end
end
