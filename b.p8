function _draw()
 if state=='game' then
  game_draw()
 elseif state=='menu' then
  menu_draw()
 elseif state=='gameover' then
  gameover_draw()
 else
  intro_draw()
 end
end

function _update()
 j+=1
 if j%10==0 then
  cl = cl==off and on or off
 end
 if state=='game' then
  game_update()
 elseif state=='menu' then
  menu_update()
 elseif state=='gameover' then
  gameover_update()
 else
  intro_update()
 end
end

function collision(o,dir,flag)
 local tile1
 local tile2
 if dir == 'down' then
  tile1 = mget((o.x+1)/8, (o.y+(o.h*8))/8)
  tile2 = mget((o.x+(o.w*8)-1)/8, (o.y+(o.h*8))/8)
 elseif dir == 'left' then
  tile1 = mget((o.x-1)/8, (o.y+1)/8)
  tile2 = mget((o.x-1)/8, (o.y+(o.h*8)-1)/8)
 elseif dir == 'right' then
  tile1 = mget((o.x+(o.w*8))/8, (o.y+1)/8)
  tile2 = mget((o.x+(o.w*8))/8, (o.y+(o.h*8)-1)/8)
 elseif dir == 'up' then
  tile1 = mget((o.x+1)/8, (o.y-1)/8)
  tile2 = mget((o.x+(o.w*8)-1)/8,(o.y-1)/8)
 end
 return fget(tile1,flag) or
  fget(tile2,flag)
end
  
function gravity(o)
 o.dy+=g
 if o.dy < 0 then
  o.jumping = true
 elseif o.dy > 0 then
  o.jumping = false
  o.falling = true
  o.dy = limit(o.dy,o.mdy)
  if collision(o, 'down', 0) then
   o.dy = 0
   o.stand = true
   o.falling = false
   o.y -= (o.y + (o.h * 8) + 1) % 8 - 1
  end
 end
 o.y+=o.dy
end

function limit(a,b)
 return mid(-b,a,b)
end

function cam(o)
 if (o.flipx and o.x < cx+24) or
 (o.flipx==false and o.x+o.w>cx+70) then
  cx+=o.dx
 end
	 if cx<=0 then
	  cx=0
	 elseif cx>=896 then
	  cx=896
	 end
end

function hit(o1,o2)
 return o1.x + (o1.w*8) >= o2.x
 and o1.y + (o1.h*8) >= o2.y
 and o1.x <= o2.x + (o2.w*8)
 and o1.y <= o2.y + (o2.h*8)
end

