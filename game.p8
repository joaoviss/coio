function game_init()
 p = player:new(8,96,3)
 enemies={} j=0
 b1=0 cx=0 cy=0 zone=40
end

function game_draw()
 cls(13)
 map(0,-2,b1,0,128,16,0x08)
 map(0,4,0,32,128,16)
 p:draw()  
 for e in all(enemies) do
  e:draw()
 end
-- print(tostr(p.inv),0,8)
 camera(cx,cy)
 draw_hud()
end

function game_update()
 i+=1 j+=1
 b1+= b1>-256 and -1 or 256
 cam(p)
 if i%40==0 then
  add(enemies, enemy:new(rnd(1024),-8,-0.1))
 end
 for e in all(enemies) do
  e:update()
  if hit(e,p) then
   if not p.inv then
	   p:hit()
   end
   if p.lives<=0 then
    state='gameover'
    camera(0,0)
	  end
  end
 end
 p:update()
 p:animate()
 updt_hud(p)
 if (p.y>128) p:die()
end
