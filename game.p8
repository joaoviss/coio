player={
 meowths={},
 cnt=0, score=0, sp=32,
 w=2, h=2, dx=0, dy=0,
 acc=3, boost=7, spd=1,
 stand=false, jumping=false, 
 falling=false, running=false,
 flipx=false, cry=false, inv=false
}

function player:new(x,y,lives)
 local o={x=x,y=y,lives=lives}
 return setmetatable(o,{__index=self}) 
end

function player:draw()
 for m in all(self.meowths) do
  m:draw()
 end
 spr(self.sp,self.x,self.y,2,2,self.flipx)
end

function player:update()
 self.dx *= f
 self.dy += g
 self.running = false
 -- move left
 if btn(‚¨ÖÔ∏è) then
  self.flipx = true
  if self.stand then
   self.running = true
  end
   self.dx -= self.acc
 end
-- move right
 if btn(‚û°Ô∏è) then
  self.flipx = false
  if self.stand then
   self.running = true
  end
  self.dx += self.acc
 end
-- jump
 if btnp(‚ùé) and self.stand then
  sfx(1)
  self.dy = -self.boost
  self.stand = false
  self.running = false
 end
-- shout
 if btn(üÖæÔ∏è) and btn(‚¨ÜÔ∏è) then
  if t==0 then
   self.cry=true
   add(self.meowths,meow:new(self.x,self.y,self.flipx,self.spd))
   t=50
  else
   t-=1
  end
 end
 if self.dx < 0 then
  self.dx = limit(self.dx,mdx)
  if collision(self, 'left', 0) or
  self.x<=0 then
   self.dx = 0
  end
 elseif self.dx > 0 then
  self.dx = limit(self.dx,mdx)
  if collision(self, 'right', 0) or
  self.x>=1008 then
   self.dx = 0
  end
 end
 gravity(self)
 self.x += self.dx
 if self.x<0 then
  self.x = 0
 end
 self.cry=false
 for m in all(self.meowths) do
  m:update()
 end
 if self.inv and j>=150 then
  self.inv=false
  j=0
 end
end

function player:animate()
 if self.running then
  if (i%5==0) sfx(2)
  if self.cnt==0 then
   self.sp = self.sp == 42 and 40 or 42
   self.cnt=3
  else
   self.cnt-=1
  end
 elseif self.jumping then
  self.sp = 40
 elseif self.falling then
  self.sp = 44
 elseif self.cry then
  self.sp = 46
 else
  if self.cnt==0 then
   self.sp += self.sp < 38 and 2 or -6
   self.cnt=5
  else
   self.cnt-=1
  end
 end
end

function player:die()
 self.lives-=1
 if self.lives<=0 then
  sfx(3)
  state='gameover'
 else
  cx=0 self.x=5 self.y=96
 end
end

function player:hit()
 self.inv=true
 self.lives-=1
 self.dx+=self.flipx and 8 or -8
 self.dy-=4
 self.x+=self.dx
 if self.lives<=0 then
  sfx(3)
  state='gameover'
 end
end
