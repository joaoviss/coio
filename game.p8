--player
player={
 cnt=0,
 score=0,
 sp=32, flipx=false,
 w=2, h=2,
 dx=0, dy=0,
 mdx=2, mdy=4.5,
 acc=3, boost=6,
 stand=false, jumping=false, 
 falling=false, running=false
}

function player:new(x,y,lives)
  local o={x=x,y=y,lives=lives}
  return setmetatable(o,{__index=self}) 
end

function player:draw()
 spr(self.sp,self.x,self.y,2,2,self.flipx)
end

function player:update()
 self.dx *= f
 self.dy += g
 self.running = false
 if btn(⬅️) then
  self.flipx = true
  if self.stand then
   self.running = true
  end
  self.dx -= self.acc
 end
 if btn(➡️) then
  self.flipx = false
  if self.stand then
   self.running = true
  end
  self.dx += self.acc
 end
 if btnp(❎) and self.stand then
  self.dy = -self.boost
  self.stand = false
  self.running = false
 end
 if self.dx < 0 then
  self.dx = limit(self.dx, self.mdx)
  if collision(self, 'left', 0) or
  self.x<=0 then
   self.dx = 0
  end
 elseif self.dx > 0 then
  self.dx = limit(self.dx, self.mdx)
  if collision(self, 'right', 0) or
  self.x>=1008 then
   self.dx = 0
  end
 end
 if self.dy < 0 then
  self.jumping = true
 elseif self.dy > 0 then
  self.jumping = false
  self.falling = true
  self.dy = limit(self.dy, self.mdy)
  if collision(self, 'down', 0) then
   self.dy = 0
   self.stand = true
   self.falling = false
   self.y -= (self.y + (self.h * 8) + 1) % 8 - 1
  end
end
 self.x += self.dx
 self.y += self.dy
 if self.x<0 then
  self.x = 0
 end
end

function player:animate()
 if self.running then
  if self.cnt==0 then
   self.sp = self.sp == 42 and 40 or 42
   self.cnt=3
  else
   self.cnt-=1
  end
 elseif self.jumping then
  self.sp = 40
 elseif self.falling then
  self.sp = 44
 else
  if self.cnt==0 then
   self.sp += self.sp < 38 and 2 or -6
   self.cnt=5
  else
   self.cnt-=1
  end
 end
end

function 	player:die()
 self.lives-=1
 if self.lives<=0 then
  state='gameover'	
 else
  cx=0 self.x=5 self.y=96
 end
end
