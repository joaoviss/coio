--word
word={}

function word:new(x,y)
 local o = {
  y=y,
  title={
   char:new(0,x),
   char:new(2,x+17),
   char:new(4,x+29),
   char:new(6,x+41),
   char:new(8,x+60),
   char:new(6,x+74),
   char:new(10,x+87),
   char:new(11,x+93),
  }
 }
 return setmetatable(o,{__index=self})
end

function word:draw()
 for c in all(self.title) do
  c:draw()
 end
end

function word:update()
 ang+=aspd
 self.title[1].y = 7 * sin(ang * (pi/180)) + self.y
 for n = #self.title,2,-1 do
  self.title[n].y = self.title[n-1].y
 end
end
