function _init()
 state='menu'
 pi=3.141519
 w=word:new(16,30)
 ang=0 curve=7 aspd=2
 f=0.8 g=0.5 i=0
 p = player:new(8,96,3)
 b1=0 cx=0 cy=0 zone=40
end

function _draw()
 if state=='menu' then
  menu_draw()
 elseif state=='game' then
  game_draw()
 elseif state=='gameover' then
  gameover_draw()
 end
end

function _update()
 if state=='menu' then
  menu_update()
 elseif state=='game' then
  game_update()
 elseif state=='gameover' then
  gameover_update()
 end
end
