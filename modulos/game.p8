 map(0,0,0,0,128,16)
 p:draw()  
 for e in all(enemies) do
  e:draw()
 end
 camera(cx,cy)
 draw_hud()
end

function game_update()
 i+=1
 b1+= b1>-256 and -1 or 256
 cam(p)
 if i%40==0 then
  add(enemies, enemy:new(rnd(1024),-8,-0.1))
 end
 for e in all(enemies) do
   e:update()
  if hit(e,p) then
   p:hit()
   del(enemies,e)
   if p.lives<=0 then
    state='gameover'
    camera(0,0)
   end
  end
 end
 p:update()
 p:animate()
 updt_hud(p)
 if (p.y>128) p:die()
end
