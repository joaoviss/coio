enemy={
 sp=32, flipx=false,
 dy=0, g=.5,
 mdx=0,mdy=4,
 w=16,h=16
}

function enemy:new(x,y,dx)
 local o={x=x, y=y, dx=dx}
 return setmetatable(o,{__index=self})
end

function enemy:draw()
 spr(self.sp,self.x,self.y,2,2,self.flipx,false)
end

function enemy:update()
 if maphit(self,'left',0)
 or maphit(self,'right',0) then
  self.dx*=-1
 end
 self.flipx=(self.dx<0)
 self.x+=self.dx
 gravity(self)
 self:animate()
end

function enemy:animate()
 if i%3==0 then
  self.sp+=self.sp<38 and 2 or -6
 end
end

function enemy:hit(o)
 return o.x+o.w>self.x
  and o.x<self.x+self.w
  and o.y+o.h>self.y
  and o.y<self.y+self.h
end
				
