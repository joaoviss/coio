   or maphit(self,'right',3) 
   or (self.stand
   and not maphit(self,'down',3,'and')) then
  self.dx*=-1
 end
 self.flipx=self.dx<0  
 self.x+=self.dx
 gravity(self)
 self:animate()
end

function rat:animate()
 if i%3==0 then
  self.sp+=self.sp<38 and 2 or -6
 end
end

