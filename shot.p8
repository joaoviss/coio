end

function _draw()
 if state=='menu' then
  menu_draw()
 elseif state=='game' then
  game_draw()
 elseif state=='gameover' then
  gameover_draw()
 end
end

function _update()
 i+=1
 if state=='menu' then
  menu_update()
 elseif state=='game' then
  hud_update()
  game_update()
 elseif state=='gameover' then
  gameover_update()
 end
end

function gravity(o)
 o.dy+=o.g
 if o.dy<0 then
  o.jumping=true
  o.stand=false
  o.running=false
  if maphit(o,'up',3) then
   o.dy=0
  end
 elseif o.dy>0 then
  o.jumping=false
  o.falling=true
  o.dy=limit(o.dy,o.mdy)
  if maphit(o,'down',3) then
   o.falling=false
   o.stand=true
   o.dy=0
   o.y-=o.y%8
  end
 end
 o.y+=o.dy
end

function maphit(o,side,flag,comp)
 local tile1,tile2
 if side=='left' then
  tile1=mget((o.x+o.dx)/8,(o.y)/8)
  tile2=mget((o.x+o.dx)/8,(o.y+o.h-1)/8)
 elseif side=='right' then
  tile1=mget((o.x+o.w+o.dx)/8,(o.y)/8)
  tile2=mget((o.x+o.w+o.dx)/8,(o.y+o.h-1)/8)
 elseif side=='up' then
  tile1=mget((o.x)/8,(o.y-1)/8)
  tile2=mget((o.x+o.w-1)/8,(o.y-1)/8)
 elseif side=='down' then
  tile1=mget((o.x)/8,(o.y+o.h+1)/8)
  tile2=mget((o.x+o.w-1)/8,(o.y+o.h+1)/8)
 elseif side=='mid' then
  tile1=mget(o.x/8,(o.y+8)/8)
  tile2=mget((o.x+o.w-1)/8,(o.y+8)/8)
 end
 if comp=='and' then
  return fget(tile1, flag) and fget(tile2, flag)
 else
  return fget(tile1, flag) or fget(tile2, flag)
 end
end

function hit(o1,o2)
 return o1.x+o1.w>o2.x
  and o1.x<o2.x+o2.w
  and o1.y+o1.h>o2.y
  and o1.y<o2.y+o2.h
end

function hud_draw()
 rectfill(cx,cy,cx+127,cy+8,5)
 rect(cx,cy,cx+127,cy+8,6)
 print(lives,cx+2,cy+2,8)
 print('fase-'..cy/128+1,cx+72,cy+2,12)
 print('$'..score,cx+106,cy+2,10)
end

function hud_update()
 local padding=''
 if p.score<10 then
  padding='00'
 elseif p.score<100 then
  padding='0'
 end
 score=padding..p.score
 lives=''
 for l=1,p.lives do
  lives=lives..'â™¥'
 end
end

function limit(a,b)
 return mid(-b,a,b)
end

function cam(o)
 local _cx=(o.x\128)*128
 if cx!=_cx then
  paused=true
  cx+=cx<_cx and 4 or -4
 else
  paused=false
 end
 camera(cx,cy)
end

function trap()
 for y=0,15 do
  for x=0,127 do
   if mget(x,y)==70 then
    mset(x,y,71)
   elseif mget(x,y)==71 then
    mset(x,y,70)
   end
  end
 end
end
