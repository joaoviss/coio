function _init()
 t=0 i=0
 state='game'
 cx=0 cy=0
 game_init()
end

function _draw()
 if state=='game' then
  game_draw()
 end  
end

function _update()
 i+=1
 if state=='game' then
  game_update()
 end
end

function gravity(o)
 o.dy+=o.g
 if o.dy<0 then
  o.jumping=true
  o.stand=false
  o.running=false
  if maphit(o,'up',0) then
   o.dy=0
  end
 elseif o.dy>0 then
  o.jumping=false
  o.falling=true
  o.dy=limit(o.dy,o.mdy)
  if maphit(o,'down',0) then
   o.falling=false
   o.stand=true
   o.dy=0
   o.y-=o.y%8
  end
 end
 o.y+=o.dy
end

function maphit(o,side,flag)
 local tile1,tile2
 if side=='left' then
   tile1=mget((o.x+o.dx)/8,(o.y)/8)
  tile2=mget((o.x+o.dx)/8,(o.y+o.h-1)/8)
 elseif side=='right' then
  tile1=mget((o.x+o.w+o.dx)/8,(o.y)/8)
  tile2=mget((o.x+o.w+o.dx)/8,(o.y+o.h-1)/8)
 elseif side=='up' then
  tile1=mget((o.x)/8,(o.y-1)/8)
  tile2=mget((o.x+o.w-1)/8,(o.y-1)/8)
 elseif side=='down' then
  tile1=mget((o.x)/8,(o.y+o.h+1)/8)
  tile2=mget((o.x+o.w-1)/8,(o.y+o.h+1)/8)
 end
 return fget(tile1, flag)
 or fget(tile2, flag)
end

function cam()
 checky=p.y
 if p.dx>0 and p.x>cx+128 then
  cx+=128
	  checkx=p.x
 elseif p.dx<0 and p.x<cx-p.w then
  cx-=128
  checkx=p.x
 end
--[[
 if (p.x<cx+16 and p.dx<0) 
 or (p.x>cx+88 and p.dx>0) then
  cx+=p.dx
 end
--]]
 if cx<0 then
  cx=0
 elseif cx>896 then
  cx=896
 end
end

function hud_draw()
 rectfill(cx,cy,cx+127,cy+8,5)
 rect(cx,cy,cx+127,cy+8,6)
 print('pontos: '..score,cx+83,cy+2,7)
 print(lives,cx+2,cy+2,8)
end

function hud_update()
 if p.score<10 then
  padding='00'
 elseif p.score<100 then
  padding='0'
 else
  padding=''
 end
 score=padding..p.score
 lives=''
 for l=1,p.lives do
 lives=lives..'â™¥'
 end
end


function status(o)
 str=''
 str=str .. 'stand: ' .. tostr(o.stand)
 str=str .. '\nrunning: ' .. tostr(o.running)
 str=str .. '\njumping: ' .. tostr(o.jumping)
 str=str .. '\nfalling: ' .. tostr(o.falling)
 str=str .. '\nshooting: ' .. tostr(o.shooting)
 print(str,cx ,cy+10,7)
end

function limit(a,b)
 return mid(-b,a,b)
end
