function bullet:new(x,y,spd,flipx)
 local o={x=x, y=y, spd=spd, flipx=flipx}
 return setmetatable(o,{__index=self})
end

function bullet:draw()
-- rect(self.x,self.y,self.x+self.w,self.y+self.h,11)
 print(self.x..' x '..self.y)
 spr(self.sp,self. x,self.y,1,1,self.flipx,false)
end

function bullet:update()
 self.x+=self.flipx and -self.spd or self.spd
 if (i%5==0) self.sp+=1
end

function bullet:hit(o)
 return self.y+self.h>o.y
 and self.y<o.y+o.h
 and self.x+self.w>o.x
 and self.x<o.x+o.w
end

