function game_init()
 p=player:new(24,104)
 checkx=24 checky=104
 bullets={} t=0 i=0
 rats={
  enemy:new(112,104,-2),
  enemy:new(96,104,2),
  enemy:new(240,104,-2),
  enemy:new(224,104,2),
 }
end

function game_draw()
 cls()
 map()
 camera(cx,cy)
 hud_draw()
 for b in all(bullets) do
  b:draw()
 end
 p:draw()
 for r in all(rats) do
  r:draw()
 end
end
 
function game_update()
 p:update()
 for r in all(rats) do
  r:update()
  for b in all(bullets) do
   if b:hit(r) then
    del(rats,r)
    del(bullets,b)
    p.score+=1
   end
  end
  if r:hit(p) and not r.inv then
   r.inv=true
   p:die()
  end
  r.inv=r:hit(p)
 end
 cam()
 for b in all(bullets) do
  b:update()
  if b.sp>b.msp then
   del(bullets,b)
  end
 end
 hud_update()
end
