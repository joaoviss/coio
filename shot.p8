snake={
 sp=44, flipx=false,
 dy=0, g=.5,
 mdx=0, mdy=4,
 w=8, h=8, inv=false
}

function snake:new(x,y,dx)
 local o={x=x, y=y, dx=dx}
 return setmetatable(o,{__index=self})
end

function snake:draw()
 spr(self.sp,self.x,self.y,1,1,self.flipx,false)
end

function snake:update()
 if self.stand
   and (maphit(self,'left',3)
   or maphit(self,'right',3) 
   or not maphit(self,'down',3,'and')) then
  self.dx*=-1
 end
 self.flipx=self.dx<0  
 if self.stand then
  self.x+=self.dx
 end
 gravity(self)
 self:animate()
end

function snake:animate()
 if i%3==0 then
  self.sp+=self.sp<47 and 1 or -3
 end
end

