--]]
 map()
 cam(p)
 hud_draw()
 for b in all(bullets) do
  b:draw()
 end
 p:draw()
 for e in all(enemies) do
  e:draw()
  
 end
end
 
function game_update()
 if not paused then
  p:update()
  for e in all(enemies) do
   e:update()
   for b in all(bullets) do
    if hit(b,e) then
     del(enemies,e)
     del(bullets,b)
     p.score+=1
    end
   end
   if hit(e,p) and not e.inv then
    sfx(4)
    p:die()
   end
   e.inv=hit(e,p) 
   if p.y>128 then
    p:die()
    cx=0
   end
  end
 end
 for b in all(bullets) do
  b:update()
  if b.sp>b.msp
  or maphit(b,'left',3)
  or maphit(b,'right',3) 
  then
   del(bullets,b)
  end
 end
 if i%30==0 then
  trap()
 end
 if maphit(p,'mid',0) and not p.inv then
--  p	.inv=true
  p:die()
-- else
--  p.inv=maphit(p,'mid',0)
 end
 p.inv=maphit(p,'mid',0)
 hud_update()
end
