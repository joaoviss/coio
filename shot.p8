player={
 bullets={}, flipx=false,
 score=0, lives=3, inv=false,
 stand=false, jumping=false, sp=0,
 dx=0, dy=0, acc=1, f=.85,
 mdx=2,mdy=4,
 g=.5, w=15, h=15, boost=6
}

function player:new(x,y)
 local o={x=x, y=y}
 return setmetatable(o,{__index=self})
end

function player:draw(x,y)
 spr(self.sp,self.x,self.y,2,2,self.flipx,false)
end

function player:update()
 self.dx*=self.f
 self.running=false
 self.shooting=false
 self:controlls()
 if (maphit(self,'left',0) and self.dx<0)
  or (maphit(self,'right',0) and self.dx>0) then
  self.x-=self.x%8
 else
  self.x+=self.dx
 end
 if (self.x<0) self.x=0
 gravity(self)
 self:animate()
 self:get_item()
end

function player:controlls()
 if btn(â¬…ï¸) then
  self.dx-=self.acc
  self.dx=limit(self.dx,self.mdx)
  self.flipx=true
  if self.stand then
    self.running=true
end  
elseif btn(âž¡ï¸) then
  self.dx+=self.acc
  self.dx=limit(self.dx,self.mdx)
  self.flipx=false
  if self.stand then
   self.running=true
  end
 end
 if btnp(âŽ) and self.stand then
  sfx(3)
  self.dy-=self.boost
 end
 if btn(ðŸ…¾ï¸) then
  if t==0 then
   self.shooting=true
   self.running=false
   self.jumping=false
   self.falling=false
   add(bullets,bullet:new(self.x+self.w/2,self.y+5,3,self.flipx))
   t=10
  else
   t-=1
  end
 end
end

function player:animate()
 if self.running then
  if i%5==0 then
   sfx(1)
   self.sp=self.sp==8 and 10 or 8
  end
 elseif self.jumping then
  self.sp=8
 elseif self.falling then
  self.sp=12
 elseif self.shooting then
  sfx(0) 
  self.sp=14
 else
  if i%5==0 then
   self.sp+=self.sp<6 and 2 or -6
  end
 end
end

function player:get_item()
 local _x={flr(self.x/8), flr((self.x+self.w)/8)}
 local _y={flr(self.y/8), flr((self.y+self.h)/8)}
 for y in all(_y) do
  for x in all(_x) do
   local flag=fget(mget(x,y))
   if flag==2 then
    sfx(6)
    mset(x,y,0)
    self.score+=1
   elseif flag==4 then
    sfx(5)
    mset(x,y,0)
    self.lives+=1
   end         
  end
 end
end

function player:die()
   sfx(4)
   self.lives-=1
   if self.lives<0 then
	   self.x=checkx
	   self.y=0	
--	   self.y=checky
    game_init()
   end
end
