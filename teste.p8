--gato coio
--por gisele henriques
function _init()
 fwrs={
  flower:new(12,15,4,10,5),
--  flower:new(12,15,10,10,5)
 }
 i = 0 f = 0.85 g = 0.6
 p = player:new(5,96)
 on=48 off=32 switch=false
 cx = 0 cy = 0 b1=0 b2=0 zone=40
end

function _draw()
 cls()
 map(4,32,b2,0,128,16)
 map(0,32,b1,0,128,16)
 map(0,16)
 map()
 camera(cx,cy)
 draw_hud()
 scan(p)
 for fwr in all(fwrs) do
  fwr:draw()
 end
 p:draw()
end

function _update()
 i += 1
 p:update()
 p:animate()
 for fwr in all(fwrs) do
 
  fwr:update()
 end
 cam(p)
 updt_hud(p)
end

