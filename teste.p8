--utilities
function limit(a,b)
 return mid(-b, a, b)
end

function cam(o)
---[[
 if (o.x<=cx+64-zone and o.dx<0) or
 (o.x+16>cx+64+zone and o.dx>0) then
  cx += o.dx
  b1-=o.dx/16
  b2-=o.dx/12
 end
--]]
--[[
 if o.x + o.dx > cx + 128 then
  cx += 128
 elseif o.x + o.dx <= cx - 8 then
  cx -= 128
 end
--]]
 if (cx<0) cx=0
 if (cx>896) cx=896
end

function collision(o,dir,flag)
 local tile1, tile2
 if dir == 'down' then
  tile1 = mget((o.x+1)/8, (o.y+(o.h*8))/8)
  tile2 = mget((o.x+(o.w*8)-1)/8, (o.y+(o.h*8))/8)
 elseif dir == 'left' then
  tile1 = mget((o.x)/8, (o.y+1)/8)
  tile2 = mget((o.x)/8, (o.y+(o.h*8)-1)/8)
  elseif dir == 'right' then
  tile1 = mget((o.x+(o.w*8))/8, (o.y+1)/8)
  tile2 = mget((o.x+(o.w*8))/8, (o.y+(o.h*8)-1)/8)
 elseif dir == 'up' then
  tile1 = mget((o.x+1)/8, (o.y-1)/8)
  tile2 = mget((o.x+(o.w*8)-1)/8,(o.y-1)/8)
 end
 
 return fget(tile1,flag) or fget(tile2,flag)
end

--function hit(o,flag)

function scan(o)
	local x=o.x/8
	for y=0,o.y/8 do
	 if fget(mget(x,y),1) then
	  for t=0,2 do
 	  mset(x+t,y,off+t)
	  end
	 end
	end
end

function draw_hud()
 rectfill(cx,0,cx+127,6,6)
 print(lives_str,cx+1,1,8)
 print(score_str,cx+88,1,1)
end

function updt_hud(o)
 lives_str=''
 if o.score<10 then
  score_str='00'
 elseif o.score<100 then
  score_str='0'
 end
  score_str = score_str .. o.score .. ' pontos'
 for l = 1,o.lives do
  lives_str = lives_str..'♥'
 end
end

