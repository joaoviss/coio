--flower
flower={dy=0,mdy=2,sp=12,w=1,h=2}
function flower:new(sp,maxsp,x,y,t)
 self.sp=sp
 self.maxsp=maxsp
 self.sc=maxsp-sp
 self.x=x*8
 self.y=y*8
 self.t=t
	return self
end

function flower:draw()
 spr(self.sp,self.x,self.y,1,2)
end

function flower:update()
 if i % self.t == 0 then
  self.sp += self.sp < self.maxsp and 1 or -self.sc
 end
 self:gravity()
end

function flower:gravity()
 self.dy += g
 if self.dy > 0 then
  self.dy = limit(self.dy, self.mdy)
  if collision(self, 'down', 0) then
   self.dy = 0
   self.y -= (self.y + (self.h * 8) + 1) % 8 - 1
  end
 end
 self.y += self.dy
end

function flower:hit(o)
 return (o.x+o.w>self.x or
 o.x<self.x+self.w) and
 (o.y+o.h>self.y or
 o.y<self.y+self.h)
end
